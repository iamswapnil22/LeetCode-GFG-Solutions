```
class Solution{
public:
    int remove_duplicate(int a[],int n){
        int count = 0;
        
        for(int i=0;i<n;i++)
        {
            if(a[count] != a[i])
            {
                count++;
                a[count] = a[i];
            }
        }
        return count+1;
        
    }
};
